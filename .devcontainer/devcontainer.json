    {
        "name": "Autonomous Cloud Security Orchestrator",
        "dockerFile": "Dockerfile",
        "customizations": {
        "vscode": {
            "extensions": [
            "ms-python.python",
            "ms-python.vscode-pylance"
            ],
            "settings": {
            "python.pythonPath": "/usr/local/bin/python3"
            }
        }
        },
        "postCreateCommand": "pip install -r server/requirements.txt && pip install -r client/requirements.txt",
        "forwardPorts": [
        5000
        ],
        "remoteUser": "vscode"
    }